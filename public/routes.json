{
    "routes": [
      {
        "route": "/dashboard",
        "allowedRoles": ["authenticated"],
        "serve": "/dashboard"
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthenticated",
        "statusCode": "302",
        "serve": "/unauthenticated.html"
      }
    ]
  }