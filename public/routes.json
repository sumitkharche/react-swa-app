{
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthenticated",
        "statusCode": "302",
        "serve": "/unauthenticated.html"
      }
    ],
    "mimeTypes": {
      "custom": "text/html"
   }
  }